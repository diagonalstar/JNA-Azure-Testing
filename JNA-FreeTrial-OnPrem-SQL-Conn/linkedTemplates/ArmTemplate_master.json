{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "JNA-FreeTrial-OnPrem-SQL-Conn"
		},
		"AzureBlobStorage_SQLcsv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_SQLcsv'"
		},
		"AzureSqlDatabase_JNA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_JNA'"
		},
		"SqlServer_OnPrem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_OnPrem'"
		},
		"SqlServer_OnPrem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer_OnPrem'"
		},
		"SqlServer_OnPrem_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Jason-Laptop\\JAsh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_SQLcsv_connectionString": {
						"value": "[parameters('AzureBlobStorage_SQLcsv_connectionString')]"
					},
					"AzureSqlDatabase_JNA_connectionString": {
						"value": "[parameters('AzureSqlDatabase_JNA_connectionString')]"
					},
					"SqlServer_OnPrem_connectionString": {
						"value": "[parameters('SqlServer_OnPrem_connectionString')]"
					},
					"SqlServer_OnPrem_password": {
						"value": "[parameters('SqlServer_OnPrem_password')]"
					},
					"SqlServer_OnPrem_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer_OnPrem_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}