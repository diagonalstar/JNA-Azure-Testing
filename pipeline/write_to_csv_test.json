									"physicalType": "int"
								},
								"sink": {
									"type": "Int32",
									"ordinal": 4
								}
							},
							{
								"source": {
									"name": "AccountDescription",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"type": "String",
									"ordinal": 5
								}
							},
							{
